#!/bin/bash
# A script to run Presentations.
ext="${@##*.}"
shopt -s nocasematch
case "$ext" in
	"prs" ) /home/1is7ac3/Git/freeoffice/presentations -S\""$@"\";;
	"pps" ) /home/1is7ac3/Git/freeoffice/presentations -S\""$@"\";;
	"ppsx" ) /home/1is7ac3/Git/freeoffice/presentations -S\""$@"\";;
	* ) /home/1is7ac3/Git/freeoffice/presentations "$@";;
esac
